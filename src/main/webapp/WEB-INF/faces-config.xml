<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	
	<navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	        <from-outcome>loginSucesso</from-outcome>
	        <to-view-id>/bemvindo.xhtml</to-view-id>
	    </navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	        <from-outcome>login</from-outcome>
	        <to-view-id>/LoginUsuario.xhtml</to-view-id>
	    </navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	        <from-outcome>login</from-outcome>
	        <to-view-id>/LoginUsuario.xhtml</to-view-id>
	    </navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	        <from-outcome>teste</from-outcome>
	        <to-view-id>/teste.xhtml</to-view-id>
	    </navigation-case>
	</navigation-rule>
	
</faces-config>
